  "version": "0.2.0",
  "configurations": [
    {
        "type": "lldb",
        "request": "launch",
        "name": "Debug unit tests in library 'cennznet-runtime'",
        "cargo": {
            "args": [
                "test",
                "--no-run",
                "--lib",
                "--package=cennznet-runtime"
            ],
            "filter": {
                "kind": "lib"
            }
        },
        "args": [],
        "cwd": "${workspaceFolder}"
    },
    {
        "type": "lldb",
        "request": "launch",
        "name": "Debug unit tests in library 'cennznet-primitives'",
        "cargo": {
            "args": [
                "test",
                "--no-run",
                "--lib",
                "--package=cennznet-primitives"
            ],
            "filter": {
                "kind": "lib"
            }
        },
        "args": [],
        "cwd": "${workspaceFolder}"
    },
    {
        "type": "lldb",
        "request": "launch",
        "name": "Debug executable 'cennznet'",
        "cargo": {
            "args": [
                "build",
                "--bin=cennznet",
                "--package=cennznet"
            ],
            "filter": {
                "kind": "bin"
            }
        },
        "args": ["--dev"],
        "cwd": "${workspaceFolder}"
    },
    {
        "type": "lldb",
        "request": "launch",
        "name": "Debug unit tests in executable 'cennznet'",
        "cargo": {
            "args": [
                "test",
                "--no-run",
                "--bin=cennznet",
                "--package=cennznet"
            ],
            "filter": {
                "kind": "bin"
            }
        },
        "args": [],
        "cwd": "${workspaceFolder}"
    }
]
